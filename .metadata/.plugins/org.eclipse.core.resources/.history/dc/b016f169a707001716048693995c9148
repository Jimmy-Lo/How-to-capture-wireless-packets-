package org.pcap4j.sample;

public class Test {
	public static void main(String args[]) {
		byte k = -9;
		  String s = Integer.toHexString(Integer.valueOf(k));
		  char[] c = s.toCharArray();
		  String r = String.valueOf(c[c.length-2]).concat(String.valueOf(c[c.length-1]));
		  System.out.println(r);
	}
}
